<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SmartERP.ModuleEditor.ReactiveUI.Views.Forms.CustomModuleFormView">

  <StackPanel>
    <Button Command="{Binding SaveButton_Click}">Save</Button>
    <Button Command="{Binding GenerateButton_Click}">Generate</Button>
    <Button Command="{Binding CloseButton_Click}">Close</Button>
    <TextBox Text="{Binding Module.Name}"></TextBox>
    <TextBox Text="{Binding Module.DisplayName}"></TextBox>
    <TextBox Text="{Binding Module.Description}"></TextBox>
    <TextBlock>Entities</TextBlock>
    
  </StackPanel>
  
</UserControl>
